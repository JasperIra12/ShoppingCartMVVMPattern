{
  "name": "act_mvvm_shop_cart",
  "version": "1.0.16",
  "private": false,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "bob build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.6",
    "react": "18.1.0",
    "react-native": "0.70.4",
    "react-redux": "^8.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-native-builder-bob": "^0.20.0",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "description": "Mini app for Shopping cart MVVM pattern",
  "main": "lib\\commonjs\\index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasperIra12/ShoppingCartMVVMPattern.git"
  },
  "keywords": [
    "[\"CustomButton\"",
    "\"CartItem\"]"
  ],
  "author": " <JasperC>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JasperIra12/ShoppingCartMVVMPattern/issues"
  },
  "homepage": "https://github.com/JasperIra12/ShoppingCartMVVMPattern#readme",
  "react-native": "src\\index.tsx",
  "module": "lib\\module\\index.js",
  "types": "lib\\typescript\\src\\index.d.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!android/build",
    "!ios/build"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "directories": {
    "lib": "lib"
  }
}
